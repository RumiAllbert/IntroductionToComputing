
public class FizzBuzz
{
    public String fizzBuzz(int n) {
        if (n % 3 == 0 && n % 5 == 0) {
            return "fizz buzz";
        }
        else if (n % 3 == 0) {
            return "fizz";
        } 

        else if (n % 5 == 0) {
            return "buzz";
        }
        else {
            return "";
        }
    }

    public static void main(String[] args) {
        for (int i = 0; i <= 100; i++) {
            FizzBuzz fb= new FizzBuzz();
            System.out.println(i + " " + fb.fizzBuzz(i));
        }
    }
}
